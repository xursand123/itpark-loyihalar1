#include <stdio.h>
#include <iostream>
using namespace std;

int main()
{
    int a = 4, b=15;
    int count = 0;
    int sum = 0;
    
    for(int i=a; i<b; i++){
      if(i % 3 == 0){
        sum = sum + i;
        count ++;    
      }
    }
    cout<<"yig'indi = "<<sum;
    cout<<"\n";
    cout<<"soni = "<<count;

    return 0;
}
